  "require": {
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5|^9.3",
    "mockery/mockery": "^1.3.1",
    "orchestra/testbench": "^4.0|^5.0|^6",
    "orchestra/database": "^4.0|^5.0|^6",
    "illuminate/support": "^5.8|^6.0|7.0|8.0",
    "fzaninotto/faker": "^1.9",
    "laravel/legacy-factories": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bitfumes\\Visits\\": "src/"
    }
  },
  "autoload-dev": {
