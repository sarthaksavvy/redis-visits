{
    "name": "bitfumes/visits",
    "description": "Your Package Description here",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Sarthak Shrivastava",
        "email": "sarthak@bitfumes.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Bitfumes\\Visits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitfumes\\Visits\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bitfumes\\Visits\\VisitsServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}